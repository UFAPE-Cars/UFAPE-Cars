<div id="<%= dom_id venda %>">
  <p>
    <strong>Cliente:</strong>
    <%= venda.cliente.nome %>
  </p>

  <p>
    <strong>Veiculo:</strong>
    <%= venda.veiculo.modelo %>
  </p>

  <p>
    <strong>Vendedor:</strong>
    <%= venda.vendedor.nome %>
  </p>

  <p>
    <strong>Valor:</strong>
    <%= venda.valor %>
  </p>

  <p>
    <strong>Quantidade veiculos:</strong>
    <%= venda.quantidade_veiculos %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= venda.status %>
  </p>

</div>
